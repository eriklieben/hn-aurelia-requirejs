{
  "name": "hn-aurelia-requirejs",
  "description": "An Aurelia client application.",
  "version": "0.1.0",
  "repository": {
    "type": "???",
    "url": "???"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "tslint": "tslint --exclude 'aurelia_project/**/*.ts' --project tsconfig.json",
    "changelog": "node ./node_modules/conventional-changelog-cli/cli.js -p angular -i CHANGELOG.md -s",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "test"
    ]
  },
  "pre-commit": "lint-staged",
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore"
    ],
    "scope": "\\S+.*"
  },
  "jest": {
    "transform": {
      ".ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "test/unit/setup.ts"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/unit/setup.ts",
    "unmockedModulePathPatterns": [
      "node_modules/aurelia-polyfills",
      "node_modules/aurelia-pal-browser"
    ],
    "collectCoverageFrom": [
      "src/**/!(*.spec.ts)"
    ],
    "coverageDirectory": "test/unit/coverage",
    "mapCoverage": true,
    "bail": true
  },
  "license": "MIT",
  "dependencies": {
    "aurelia-animator-css": "^1.0.2",
    "aurelia-bootstrapper": "^2.1.1",
    "bluebird": "^3.4.1",
    "moment": "^2.18.1",
    "requirejs": "^2.3.2",
    "text": "github:requirejs/text#latest",
    "tslib": "^1.7.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "@types/node": "^6.0.45",
    "aurelia-cli": "^0.30.0",
    "aurelia-pal-nodejs": "^1.0.0-beta.1.0.0",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "aurelia-tools": "^1.0.0",
    "autoprefixer": "^6.3.6",
    "browser-sync": "^2.13.0",
    "commitplease": "^2.7.10",
    "connect-history-api-fallback": "^1.2.0",
    "conventional-changelog-cli": "^1.3.1",
    "debounce": "^1.0.2",
    "event-stream": "^3.3.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed-in-place": "^2.0.3",
    "gulp-htmlmin": "^3.0.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^3.1.4",
    "gulp-watch": "^4.3.11",
    "gulp-zip": "^4.0.0",
    "html-minifier": "^3.2.3",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "minimatch": "^3.0.2",
    "postcss-cssnext": "^2.11.0",
    "postcss-modules": "^0.8.0",
    "pre-commit": "^1.2.2",
    "through2": "^2.0.1",
    "ts-jest": "^20.0.6",
    "tslint": "^3.15.1",
    "typescript": ">=1.9.0-dev || ^2.0.0",
    "uglify-js": "^2.6.3",
    "vinyl-fs": "^2.4.3",
    "firebase": "^4.1.3",
    "promise-polyfill": "^6.0.2",
    "workbox-build": "^1.0.1"
  }
}